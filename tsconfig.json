{
  "compileOnSave": true,
  "compilerOptions": {
    "lib": ["es2016", "dom"],
    "moduleResolution": "node",
    "module": "commonjs",
    "noImplicitAny": true,
    "listEmittedFiles": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "target": "es2015",
    "forceConsistentCasingInFileNames": true
  }
}
